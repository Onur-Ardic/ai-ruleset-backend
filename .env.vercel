# Vercel Environment Variables
# Bu değerleri Vercel dashboard'da Environment Variables bölümünde tanımlayın

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Gemini Configuration  
GEMINI_API_KEY=your_gemini_api_key_here

# Hugging Face Configuration
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# App Configuration
DEBUG=false
ENVIRONMENT=production

# Vercel'de bu değerleri nasıl ayarlayacağınız:
# 1. Vercel Dashboard'a gidin
# 2. Projenizi seçin
# 3. Settings > Environment Variables'a gidin
# 4. Her bir değişkeni ayrı ayrı ekleyin
# 5. Production, Preview, Development ortamları için gerekli olanları seçin
